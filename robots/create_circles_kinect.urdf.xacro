<?xml version="1.0"?>
<!--
    - Base      : create
    - Stacks    : circles
    - 3d Sensor : kinect
-->    
<robot name="turtlebot" xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:include filename="$(find gatlin)/urdf/turtlebot_library.urdf.xacro" />

  <create/>
  <stack_circles parent="base_link"/>
  <sensor_kinect  parent="base_link" >
    <!-- <origin xyz="${cam_px} 0 ${cam_pz}" rpy="0 0 0" /> -->
    <origin xyz="${cam_px} 0 .6" rpy="0 .4 0" />
  </sensor_kinect>
</robot>