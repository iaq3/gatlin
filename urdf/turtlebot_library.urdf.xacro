  <!-- Stacks -->
  <xacro:include filename="$(find gatlin)/urdf/stacks/circles.urdf.xacro"/>
  <xacro:include filename="$(find gatlin)/urdf/stacks/squares.urdf.xacro"/>
  <!-- <xacro:include filename="$(find turtlebot_description)/urdf/stacks/hexagons.urdf.xacro"/> -->
  <!-- 3D Sensors -->
